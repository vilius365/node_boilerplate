  "main": "index.js",
  "scripts": {
    "build:prod": "npm run wp-build && npm run do:postbuild",
    "do:postbuild": "copyfiles ./package.json ./dist/ && copyfiles ./%npm_package_config_packageName%.conf-template ./dist/",
    "dev": "nodemon",
    "version:prepatch": "npm version prepatch && npm ci",
    "version:patch": "npm version patch && npm ci",
    "version:minor": "npm version minor && npm ci",
    "version:major": "npm version major && npm ci",
    "printVersion": "echo Version: %npm_package_version% built on %date% %time%",
    "wp-build": "npm version prepatch && tsc && webpack --config webpack.prod.js --progress &&  npm run printVersion "
  },
